{
  "manifest_version": 3,
  "name": "Begone History",
  "version": "1.4",
  "description": "Deletes all cookies (except for selected domains) and history after chrome closes.",
  "permissions": [
    "windows",
    "browsingData"
  ],
  "background": {
    "service_worker": "service-worker.js"
  },
  "icons":{
    "128": "icon_small.png"
  },
  "action": {
     "default_icon": "icon.png",
     "default_title": "Begone History",
     "default_popup": "popup.html"
  },
  "offline_enabled": true
}
