{
  "name": "Begone History",
  "version": "1.2",
  "description": "Deletes all cookies (except for selected domains) and history after chrome closes.",
  "permissions": [
    "windows",
    "browsingData"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons":{
    "128": "icon_small.png"
  },
  "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "offline_enabled": true
}
